<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.test.scrolltransptoolbar">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".VelocityTrackerTestActivity"/>
        <activity android:name=".SorollToolbartranActivity"/>
        <activity android:name="com.test.scrolltransptoolbar.SuspensionEffectActivity"/>
        <activity android:name="com.test.scrolltransptoolbar.RecyclerviewActivity"/>
        <activity android:name="com.test.scrolltransptoolbar.ViewFlipperActivity"/>
        <activity android:name="com.test.scrolltransptoolbar.ViewFlipperRollActivity"/>
        <activity android:name="com.test.scrolltransptoolbar.ViewPagerPageTransformerActivity"/>
        <activity android:name="com.test.scrolltransptoolbar.ViewPagerCardViewActivity"/>
    </application>

</manifest>